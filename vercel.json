{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/public-api/batch-enrich",
      "dest": "api/batch-enrich.js"
    },
    {
      "src": "/public-api/batch-enrich-company-name-fallback",
      "dest": "api/batch-enrich-company-name-fallback.js"
    },
    {
      "src": "/public-api/batch-enrich-franchise",
      "dest": "api/batch-enrich-franchise.js"
    },
    {
      "src": "/public-api/batch-clean-titles-and-locations",
      "dest": "api/batch-clean-titles-and-locations.js"
    },
    {
      "src": "/public-api/getcarbrands",
      "dest": "api/getcarbrands.js"
    }
  ]
}
